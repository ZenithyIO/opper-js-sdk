{
  "name": "@opper/core",
  "version": "0.2.0",
  "description": "Opper SDK for JavaScript/TypeScript.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "opper",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "exports": {
