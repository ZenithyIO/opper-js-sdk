      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opper-devkit/opper-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/opper-devkit/opper-js-sdk/issues"
  },
  "homepage": "https://github.com/opper-devkit/opper-js-sdk",
  "keywords": [
    "opper",
    "opper-sdk"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ngify/types": "^1.5.2"
  },
  "peerDependencies": {
    "miniprogram-api-typings": "^3.10.0",
    "rxjs": "^7.8.1"
  }
