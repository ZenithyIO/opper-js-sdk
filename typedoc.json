{
  "$schema": "https://typedoc.org/schema.json",
  "name": "opper",
  "out": "docs",
  "entryPoints": [
    "packages/*"
  ],
  "entryPointStrategy": "packages",
  "exclude": [
    "dist",
    "packages/**/*.spec.ts",
  ],
  "excludeExternals": true,
  "excludeInternal": true,
  "githubPages": true,
  "readme": "./README.md",
  "categorizeByGroup": false
}
